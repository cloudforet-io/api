          "description": "",
          "methods": [
            {
              "name": "create",
              "description": "desc: Creates a new Quota limiting the use of a selected Protocol for a day or a month. If the parameter `limit` has no value, it will be deemed unlimited. If a Protocol has not set a Quota, the default Quota set in the Config will be applied.\nrequest_example: \u003e-\n{\n\"protocol_id\": \"protocol-123456789012\",\n\"limit\": {\n\"day\": 5.0,\n\"month\": 7.0\n}\n}\nresponse_example: \u003e-\n{\n\"quota_id\": \"quota-123456789012\",\n\"protocol_id\": \"protocol-123456789012\",\n\"limit\": {\n\"day\": 5.0,\n\"month\": 7.0\n},\n\"domain_id\": \"domain-123456789012\"\n}",
              "requestType": "CreateQuotaRequest",
              "requestLongType": "CreateQuotaRequest",
              "requestFullType": "spaceone.api.notification.v1.CreateQuotaRequest",
              "requestStreaming": false,
              "responseType": "QuotaInfo",
              "responseLongType": "QuotaInfo",
              "responseFullType": "spaceone.api.notification.v1.QuotaInfo",
              "responseStreaming": false,
              "options": {
                "google.api.http": {
                  "rules": [
                    {
                      "method": "POST",
                      "pattern": "/notification/v1/quota/create",
                      "body": "*"
                    }
                  ]
                }
              }
            },
            {
              "name": "update",
              "description": "desc: Updates a specific Quota. You can make changes in Quota `limit`, managing the use of the Protocol.\nrequest_example: \u003e-\n{\n\"quota_id\": \"quota-123456789012\",\n\"limit\": {\n\"day\": 10.0,\n\"month\": 15.0\n}\n}\nresponse_example: \u003e-\n{\n\"quota_id\": \"quota-123456789012\",\n\"protocol_id\": \"protocol-123456789012\",\n\"limit\": {\n\"day\": 10.0,\n\"month\": 15.0\n},\n\"domain_id\": \"domain-123456789012\"\n}",
              "requestType": "UpdateQuotaRequest",
              "requestLongType": "UpdateQuotaRequest",
              "requestFullType": "spaceone.api.notification.v1.UpdateQuotaRequest",
              "requestStreaming": false,
              "responseType": "QuotaInfo",
              "responseLongType": "QuotaInfo",
              "responseFullType": "spaceone.api.notification.v1.QuotaInfo",
              "responseStreaming": false,
              "options": {
                "google.api.http": {
                  "rules": [
                    {
                      "method": "POST",
                      "pattern": "/notification/v1/quota/update",
                      "body": "*"
                    }
                  ]
                }
              }
            },
            {
              "name": "delete",
              "description": "desc: Deletes a specific Quota. The default Quota set in the Config will be applied to the Protocol you deleted the Quota of.\nrequest_example: \u003e-\n{\n\"quota_id\": \"quota-123456789012\"\n}",
              "requestType": "QuotaRequest",
              "requestLongType": "QuotaRequest",
              "requestFullType": "spaceone.api.notification.v1.QuotaRequest",
              "requestStreaming": false,
              "responseType": "Empty",
              "responseLongType": ".google.protobuf.Empty",
              "responseFullType": "google.protobuf.Empty",
              "responseStreaming": false,
              "options": {
                "google.api.http": {
                  "rules": [
                    {
                      "method": "POST",
                      "pattern": "/notification/v1/quota/delete",
                      "body": "*"
                    }
                  ]
                }
              }
            },
            {
              "name": "get",
              "description": "desc: Gets a specific Quota. Prints detailed information about the Quota, including the `limit` and the Protocol limited by the Quota.\nrequest_example: \u003e-\n{\n\"quota_id\": \"quota-123456789012\"\n}\nresponse_example: \u003e-\n{\n\"quota_id\": \"quota-123456789012\",\n\"protocol_id\": \"protocol-123456789012\",\n\"limit\": {\n\"day\": 10.0,\n\"month\": 15.0\n},\n\"domain_id\": \"domain-123456789012\"\n}",
              "requestType": "QuotaRequest",
              "requestLongType": "QuotaRequest",
              "requestFullType": "spaceone.api.notification.v1.QuotaRequest",
              "requestStreaming": false,
              "responseType": "QuotaInfo",
              "responseLongType": "QuotaInfo",
              "responseFullType": "spaceone.api.notification.v1.QuotaInfo",
              "responseStreaming": false,
              "options": {
                "google.api.http": {
                  "rules": [
                    {
                      "method": "POST",
                      "pattern": "/notification/v1/quota/get",
                      "body": "*"
                    }
                  ]
                }
              }
            },
            {
              "name": "list",
              "description": "desc: Gets a list of all Quotas. You can use a query to get a filtered list of Quotas.\nrequest_example: \u003e-\n{\n\"query\": {}\n}\nresponse_example: \u003e-\n{\n\"results\": [\n{\n\"quota_id\": \"quota-123456789012\",\n\"protocol_id\": \"protocol-123456789012\",\n\"limit\": {\n\"day\": 10.0,\n\"month\": 15.0\n},\n\"domain_id\": \"domain-123456789012\"\n},\n{\n\"quota_id\": \"quota-987654321098\",\n\"protocol_id\": \"protocol-987654321098\",\n\"limit\": {\n\"day\": 5.0,\n\"month\": 7.0\n},\n\"domain_id\": \"domain-123456789012\"\n}\n],\n\"total_count\": 2\n}",
              "requestType": "QuotaQuery",
              "requestLongType": "QuotaQuery",
              "requestFullType": "spaceone.api.notification.v1.QuotaQuery",
              "requestStreaming": false,
              "responseType": "QuotasInfo",
              "responseLongType": "QuotasInfo",
              "responseFullType": "spaceone.api.notification.v1.QuotasInfo",
              "responseStreaming": false,
              "options": {
                "google.api.http": {
                  "rules": [
                    {
                      "method": "POST",
                      "pattern": "/notification/v1/quota/list",
                      "body": "*"
                    }
                  ]
                }
              }
            },
