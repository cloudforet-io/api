          "methods": [
            {
              "name": "create",
              "description": "desc: Creates a new Board with SYSTEM permission. The `name` of the board is only required. You can add one or more `categories` representing the Board's attributes.\nrequest_example: \u003e-\n{\n\"name\": \"notice\",\n\"categories\": [\"admin\", \"developer\", \"devops\"],\n\"tags\": {\"a\": \"b\"}\n}\nresponse_example: \u003e-\n{\n\"board_id\": \"board-123456789012\",\n\"name\": \"notice\",\n\"categories\": [\n\"admin\",\n\"developer\",\n\"devops\"\n],\n\"tags\": {\n\"a\": \"b\"\n},\n\"created_at\": \"2022-01-01T06:47:27.759Z\"\n}",
              "requestType": "CreateBoardRequest",
              "requestLongType": "CreateBoardRequest",
              "requestFullType": "spaceone.api.board.v1.CreateBoardRequest",
              "requestStreaming": false,
              "responseType": "BoardInfo",
              "responseLongType": "BoardInfo",
              "responseFullType": "spaceone.api.board.v1.BoardInfo",
              "responseStreaming": false,
              "options": {
                "google.api.http": {
                  "rules": [
                    {
                      "method": "POST",
                      "pattern": "/board/v1/boards"
                    }
                  ]
                }
              }
            },
            {
              "name": "update",
              "description": "desc: Updates a specific Board with SYSTEM permission. You can make changes in Board settings, including `name` and `tags`.\nrequest_example: \u003e-\n{\n\"board_id\": \"board-123456789012\",\n\"name\": \"system notice\",\n\"tags\": {\"b\": \"c\"}\n}\nresponse_example: \u003e-\n{\n\"board_id\": \"board-123456789012\",\n\"name\": \"system notice\",\n\"categories\": [\n\"admin\",\n\"developer\",\n\"devops\"\n],\n\"tags\": {\n\"b\": \"c\"\n},\n\"created_at\": \"2022-01-01T06:47:27.759Z\"\n}",
              "requestType": "UpdateBoardRequest",
              "requestLongType": "UpdateBoardRequest",
              "requestFullType": "spaceone.api.board.v1.UpdateBoardRequest",
              "requestStreaming": false,
              "responseType": "BoardInfo",
              "responseLongType": "BoardInfo",
              "responseFullType": "spaceone.api.board.v1.BoardInfo",
              "responseStreaming": false,
              "options": {
                "google.api.http": {
                  "rules": [
                    {
                      "method": "PUT",
                      "pattern": "/board/v1/board/{board_id}"
                    }
                  ]
                }
              }
            },
            {
              "name": "set_categories",
              "description": "desc: Adds or changes `categories` of a specific Board with SYSTEM permission. A change in `categories` of a Board does not affect the `category` of the child Posts.\nrequest_example: \u003e-\n{\n\"board_id\": \"board-123456789012\",\n\"categories\": [\"Developer\", \"SRE\", \"Devops\"]\n}\nresponse_example: \u003e-\n{\n\"board_id\": \"board-123456789012\",\n\"name\": \"dev-notice\",\n\"categories\": [\n\"Developer\",\n\"SRE\",\n\"Devops\",\n],\n\"tags\": {\n\"b\": \"c\"\n},\n\"created_at\": \"2022-01-01T05:24:19.758Z\"\n}",
              "requestType": "SetBoardCategoriesRequest",
              "requestLongType": "SetBoardCategoriesRequest",
              "requestFullType": "spaceone.api.board.v1.SetBoardCategoriesRequest",
              "requestStreaming": false,
              "responseType": "BoardInfo",
              "responseLongType": "BoardInfo",
              "responseFullType": "spaceone.api.board.v1.BoardInfo",
              "responseStreaming": false,
              "options": {
                "google.api.http": {
                  "rules": [
                    {
                      "method": "PUT",
                      "pattern": "/board/v1/board/{board_id}/set-categories"
                    }
                  ]
                }
              }
            },
            {
              "name": "delete",
              "description": "desc: Deletes a specific Board with `SYSTEM` permission. You can delete a Board regardless of the presence of Posts created under the Board.\nrequest_example: \u003e-\n{\n\"board_id\": \"board-123456789012\"\n}",
              "requestType": "BoardRequest",
              "requestLongType": "BoardRequest",
              "requestFullType": "spaceone.api.board.v1.BoardRequest",
              "requestStreaming": false,
              "responseType": "Empty",
              "responseLongType": ".google.protobuf.Empty",
